# 環境設定ファイル例
# GAP Tracker アプリケーション

# React アプリケーション設定
REACT_APP_NAME=GAP Tracker
REACT_APP_VERSION=1.0.0

# Firebase 設定
REACT_APP_FIREBASE_API_KEY=your_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_auth_domain_here
REACT_APP_FIREBASE_PROJECT_ID=your_project_id_here
REACT_APP_FIREBASE_STORAGE_BUCKET=your_storage_bucket_here
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
REACT_APP_FIREBASE_APP_ID=your_app_id_here

# ロギング設定
REACT_APP_LOG_LEVEL=DEBUG
REACT_APP_ENABLE_CONSOLE_LOGGING=true
REACT_APP_ENABLE_REMOTE_LOGGING=false

# エラートラッキング設定（将来的な実装用）
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
REACT_APP_ENABLE_ERROR_TRACKING=false

# 本番環境設定
REACT_APP_PRODUCTION_URL=https://your-production-url.com
REACT_APP_ENABLE_ANALYTICS=false